<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.UserInfoViewController">
	<center>
		<BorderPane prefHeight="383.0" prefWidth="419.0" BorderPane.alignment="CENTER">
			<top>
				<Label text="User Information" BorderPane.alignment="CENTER">
					<font>
						<Font name="Times New Roman Bold" size="30.0" />
					</font>
				</Label>
			</top>
			<center>
				<Pane prefHeight="292.0" prefWidth="345.0" BorderPane.alignment="CENTER">
					<children>
						<Label layoutX="173.0" layoutY="54.0" text="Username">
							<font>
								<Font name="Times New Roman" size="18.0" />
							</font>
						</Label>
						<Label layoutX="174.0" layoutY="127.0" text="Password">
							<font>
								<Font name="Times New Roman" size="18.0" />
							</font>
						</Label>
						<TextField fx:id="UsernameInput" layoutX="125.0" layoutY="84.0" prefHeight="37.0" prefWidth="167.0" promptText="Enter a username">
                     <font>
                        <Font name="Times New Roman" size="20.0" />
                     </font></TextField>
						<TextField fx:id="PasswordInput" layoutX="125.0" layoutY="156.0" onAction="#EnterButtonClicked" prefHeight="37.0" prefWidth="167.0" promptText="Enter a password">
                     <font>
                        <Font name="Times New Roman" size="20.0" />
                     </font></TextField>
						<Label layoutX="13.0" layoutY="4.0" text="Please Enter A Username and Password for your Account">
							<font>
								<Font name="Times New Roman" size="17.0" />
							</font>
						</Label>
						<Button fx:id="ResetButton" layoutX="55.0" layoutY="206.0" mnemonicParsing="false" onAction="#ResetButtonClicked" prefHeight="42.0" prefWidth="81.0" text="Reset" />
						<Button fx:id="EnterButton" layoutX="282.0" layoutY="206.0" mnemonicParsing="false" onAction="#EnterButtonClicked" prefHeight="42.0" prefWidth="81.0" text="Enter" />
						<Label fx:id="ActionTextOutput" layoutX="2.0" layoutY="276.0" prefHeight="16.0" prefWidth="419.0">
							<font>
								<Font name="Times New Roman Italic" size="14.0" />
							</font>
						</Label>
					</children>
				</Pane>
			</center>
		</BorderPane>
	</center>
</BorderPane>
